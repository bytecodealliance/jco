{
    "$schema": "./node_modules/oxlint/configuration_schema.json",
    "plugins": [],
    "categories": {
        "correctness": "off"
    },
    "env": {
        "builtin": true
    },
    "ignorePatterns": ["test/output/**/*.js"],
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "curly": ["error", "all"]
    }
}
